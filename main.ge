
ფუნქცია თქვი_გამარჯობა()
{
    ბეჭდვა("გამარჯობა სამყარო\n");

    ი = 0;

    სანამ(ი < 10)
    {
        ბეჭდვა("გამარჯობა სამყარო\n");
        თუ(ი == 5)
        {
            ბეჭდვა("ი არის 5_ი\n");
        }
        ი++;
    }
}

ფუნქცია მთავარი()
{
    თქვი_გამარჯობა();
    დააბრუნე(0);
}